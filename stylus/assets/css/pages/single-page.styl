.single-page
  .content-module
    min-height 600px
    
  .icon-glyphlogo-new
    display none!important //hide big logo

  #main, .footer
    max-width 60em
    margin 0 auto
    width 95%

  #main
    margin 3em auto!important

  .footer
    background none
    height auto
    // width 100%
    min-height auto
    padding 0
    font-size 1em

    div
      position static

  .page-content,
  .page-heading
    width 100%

  //specific page overrides
  #register
    margin-top 10em!important

    h3.nav-section
      border-top 0 none
  #team, #contact
    background white
    color color-text-gray
    div, p, a, h3
      color: color-text-gray
      border-color: color-text-gray

